{
  "expo": {
    "name": "marlin-app",
    "slug": "drex",
    "scheme": "marlin-app",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#015DEC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Marlin"
    },
    "android": {
      "versionCode": 6,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/marlinLogoApp.png",
        "backgroundColor": "#1952BE"
      },
      "package": "com.luisda1828.marlinapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "marlin-app",
              "host": "reset-password"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7f85d46b-797c-4b9d-8a4f-56159d473c9c"
      }
    },
    "owner": "nextgendevs"
  }
}
